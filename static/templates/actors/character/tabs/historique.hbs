<div class="tab historique item-container" data-container-type="actorHistorique" data-group="primary"
    data-tab="historique">
    <section class="personnage-biographie">
        <div class="nationalite-container">
            <div class="nationalite">
                <label>{{localize "EXITNIHILO.Personnage.Nationalite.Titre"}}</label>
                <input name="system.details.biographie.nationalite.value" type="text"
                    data-dtype="{{actor.system.details.biographie.nationalite.type}}"
                    value="{{actor.system.details.biographie.nationalite.value}}"
                    placeholder="{{localize 'EXITNIHILO.Personnage.Nationalite.PlaceHolder'}}" size="10" />
            </div>
        </div>        
        <div class="lieu-de-naissance-container">
            <div class="lieu-de-naissance">
                <label>{{localize "EXITNIHILO.Personnage.Lieu-De-Naissance.Titre"}}</label>
                <input name="system.details.biographie.lieu-de-naissance.value" type="text"
                    data-dtype="{{actor.system.details.biographie.lieu-de-naissance.type}}"
                    value="{{actor.system.details.biographie.lieu-de-naissance.value}}"
                    placeholder="{{localize 'EXITNIHILO.Personnage.Lieu-De-Naissance.PlaceHolder'}}" size="10" />
            </div>
        </div>
        <div class="slogans-container">
            <div class="slogans">
                <label>{{localize "EXITNIHILO.Personnage.Slogans.Titre"}}</label>
                <input name="system.details.biographie.slogans.value" type="text"
                    data-dtype="{{actor.system.details.biographie.slogans.type}}"
                    value="{{actor.system.details.biographie.slogans.value}}"
                    placeholder="{{localize 'EXITNIHILO.Personnage.Slogans.PlaceHolder'}}" size="10" />
            </div>
        </div>
        <div class="croyances-container">
            <div class="croyances">
                <label>{{localize "EXITNIHILO.Personnage.Croyances.Titre"}}</label>
                <input name="system.details.biographie.croyances.value" type="text"
                    data-dtype="{{actor.system.details.biographie.croyances.type}}"
                    value="{{actor.system.details.biographie.croyances.value}}"
                    placeholder="{{localize 'EXITNIHILO.Personnage.Croyances.PlaceHolder'}}" size="10" />
            </div>
        </div>
        <div class="gouts-container">
            <div class="gouts">
                <label>{{localize "EXITNIHILO.Personnage.Gouts.Titre"}}</label>
                <input name="system.details.biographie.gouts.value" type="text"
                    data-dtype="{{actor.system.details.biographie.gouts.type}}"
                    value="{{actor.system.details.biographie.gouts.value}}"
                    placeholder="{{localize 'EXITNIHILO.Personnage.Gouts.PlaceHolder'}}" size="10" />
            </div>
        </div>
        <div class="aversions-container">
            <div class="aversions">
                <label>{{localize "EXITNIHILO.Personnage.Aversions.Titre"}}</label>
                <input name="system.details.biographie.aversions.value" type="text"
                    data-dtype="{{actor.system.details.biographie.aversions.type}}"
                    value="{{actor.system.details.biographie.aversions.value}}"
                    placeholder="{{localize 'EXITNIHILO.Personnage.Aversions.PlaceHolder'}}" size="10" />
            </div>
        </div>
        <div class="allies-container">
            <div class="allies">
                <label>{{localize "EXITNIHILO.Personnage.Allies.Titre"}}</label>
                <input name="system.details.biographie.allies.value" type="text"
                    data-dtype="{{actor.system.details.biographie.allies.type}}"
                    value="{{actor.system.details.biographie.allies.value}}"
                    placeholder="{{localize 'EXITNIHILO.Personnage.Allies.PlaceHolder'}}" size="10" />
            </div>
        </div>
        <div class="ennemis-container">
            <div class="ennemis">
                <label>{{localize "EXITNIHILO.Personnage.Ennemis.Titre"}}</label>
                <input name="system.details.biographie.ennemis.value" type="text"
                    data-dtype="{{actor.system.details.biographie.ennemis.type}}"
                    value="{{actor.system.details.biographie.ennemis.value}}"
                    placeholder="{{localize 'EXITNIHILO.Personnage.Ennemis.PlaceHolder'}}" size="10" />
            </div>
        </div>
        <div class="organisation-container">
            <div class="organisation">
                <label>{{localize "EXITNIHILO.Personnage.Organisation.Titre"}}</label>
                <input name="system.details.biographie.organisation.value" type="text"
                    data-dtype="{{actor.system.details.biographie.organisation.type}}"
                    value="{{actor.system.details.biographie.organisation.value}}"
                    placeholder="{{localize 'EXITNIHILO.Personnage.Organisation.PlaceHolder'}}" size="10" />
            </div>
        </div>
        <div class="apparence-container">
            <label>{{localize "EXITNIHILO.Personnage.Apparence.Titre"}}</label>
            <div class="apparence">
                {{editor enrichedContent.apparence target="system.details.biographie.apparence.value"
                engine="prosemirror" button=true owner=owner editable=editable collaborate=true}}
            </div>
        </div>
        <div class="histoire-container">
            <div class="histoire">
                <label>{{localize "EXITNIHILO.Personnage.Histoire.Titre"}}</label>
                {{editor enrichedContent.histoire target="system.details.biographie.histoire.value"
                engine="prosemirror" button=true owner=owner editable=editable collaborate=true}}
            </div>
        </div>
        <div class="notes-container">
            <label>{{localize "EXITNIHILO.Personnage.Notes.Titre"}}</label>
            <div class="notes-diverses">
                {{editor enrichedContent.notes target="system.details.biographie.notes.value"
                engine="prosemirror" button=true owner=owner editable=editable collaborate=true}}
            </div>
        </div>
    </section>
</div>